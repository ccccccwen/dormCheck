/* pages/photocheck/photocheck.wxss */
.index_page{
  margin-top: 60px;
}
label{
  margin-left: 25%;
}
.picker{
  background-color: rgb(255, 255, 255);
  border: solid rgb(88, 88, 88) 1rpx;
  color: rgb(44, 43, 43);
  font-size: 16px;
  text-align: center;
  border-radius: 10rpx;
  margin-bottom: 15px;
  margin-left: 25%;
  width:400rpx;
  height:60rpx;
}
input{
  border: solid rgb(88, 88, 88) 1rpx;
  color: rgb(44, 43, 43);
  font-size: 16px;
  text-align: center;
  margin-left: 25%;
  border-radius: 10rpx;
  margin-bottom: 15px;
  width:400rpx;
  height:60rpx;
}
.camera {
  width: 400rpx;
  height: 400rpx;
  border-radius: 50%;
  margin: 20px auto 0;
  margin-bottom: 15px;
  position: relative;
}

.camera image {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 10;
}

.camera camera {
  width: 400rpx;
  height: 400rpx;
}
.photo{
  background-color: rgb(49, 207, 147);
}

