/* pages/photocheck/photocheck.wxss */
.index_page{
  margin-top: 60px;
}
.picker{
  background-color: rgb(255, 255, 255);
  color: rgb(44, 43, 43);
  font-size: 20px;
  text-align: center;
  border-radius: 10rpx;
  width:400rpx;
  height:55rpx;
}
.arrow-icon {
  width: 18px;
  height: 18px;
  margin-left: 100%;
}
input{
  margin-bottom: 10px;
  color: rgb(44, 43, 43);
  font-size: 20px;
  text-align: center;
  border-radius: 10rpx;
  width:400rpx;
  height:55rpx;
}
.camera {
  width: 400rpx;
  height: 400rpx;
  border-radius: 50%;
  margin: 20px auto 0;
  margin-bottom: 15px;
  position: relative;
}

.camera image {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 10;
}

.camera camera {
  width: 400rpx;
  height: 400rpx;
}
.photo{
  background-color: rgb(49, 207, 147);
}

