.title{
  text-align: center;
  font-size: 20px;
  margin-top: 10px;
  margin-bottom: 20px;
}
.table {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.tableX {
  border: 1px solid #dadada;
  border-right: 0;
  border-bottom: 0;
  width: 93%;
}
.tr, .tableY {
  width: 1400rpx;
  display: flex;
  justify-content: flex-start;
}
.th1,.td1{
  width: 100rpx;
  padding: 10px 0;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
}
.th,.td {
  width: 200rpx;
  padding: 10px 0;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
}
.th {
  font-weight: bold;
}
.picker{
  background-color: rgb(255, 255, 255);
  border: solid rgb(88, 88, 88) 1rpx;
  color: rgb(44, 43, 43);
  font-size: 16px;
  text-align: center;
  border-radius: 10rpx;
  margin-bottom: 15px;
  margin-left: 25%;
  width:300rpx;
  height:60rpx;
}
label{
  margin-left: 25%;
}
